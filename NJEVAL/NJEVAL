njeval.obj:	njeval.c
	njpl -t cl "/Ox /DLINT_ARGS /c njeval.c"

njevalfn.obj:	njevalfn.c
	njpl -t cl "/Ox /DLINT_ARGS /c njevalfn.c"

njeval.exe:	njeval.obj njevalfn.obj
	njpl -t link "njeval+njevalfn,njeval.exe,njeval.map /map /noi /exepack,\lib\snjlib;"

